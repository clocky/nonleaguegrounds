{
  "name": "nonleaguegrounds-local",
  "version": "1.2.0",
  "description": "Database of British non-league grounds",
  "main": "index.html",
  "scripts": {
    "start": "ELEVENTY_ENV=development eleventy --serve --incremental --quiet",
    "build": "ELEVENTY_ENV=production DEBUG=Eleventy* eleventy",
    "postbuild": "npm-run-all -s postbuild:*",
    "postbuild:prettier": "prettier --write 'dist/**/*.html'",
    "postbuild:validate": "html-validate --config .htmlvalidate.json dist"
  },
  "keywords": [
    "football",
    "soccer",
    "uk"
  ],
  "author": "Mark McLaughlin",
  "license": "MIT",
  "dependencies": {
    "netlify-plugin-cache": "^1.0.3"
  },
  "packageManager": "pnpm@8.1.0",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@sanity/client": "^5.4.2",
    "@vizuaalog/bulmajs": "^0.12.2",
    "bulma": "^0.9.4",
    "comma-number": "^2.1.0",
    "dayjs": "^1.11.7",
    "distance-from": "^1.1.2",
    "dotenv": "^16.0.3",
    "html-minifier": "^4.0.0",
    "html-validate": "^7.15.1",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "ordinal": "^1.0.3",
    "prettier-cli": "^0.1.0",
    "sass": "^1.61.0",
    "sharp": "^0.32.0",
    "start-server-and-test": "^2.0.0",
    "svgo": "^3.0.2"
  }
}