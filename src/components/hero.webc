<template webc:nokeep>
  <section class="hero" :class="this.class" :id="id" webc:root="override">
    <div class="hero-body">
      <div class="container is-max-desktop has-text-centered px-5">
        <h1 class="title is-size-1">
          <slot webc:nokeep name="title"></slot>
        </h1>
        <h2 class="subtitle mt-5">
          <slot webc:nokeep name="description"></slot>
        </h2>
      </div>
    </div>
  </section>
</template>
