{
    "extends": ["web-recommended"],
    "hints": {
        "axe/color": ["default", {"color-contrast-enhanced": "off"}],
        "http-compression": "off",
        "compat-api/css": ["error", {"ignore": ["text-size-adjust", "-webkit-tap-highlight-color"]}],
    },
    "ignoredUrls": [{
        "domain": ".*\\.googleapis\\.com/.*",
        "hints": ["*"]
    },
    {
        "domain": ".*\\.gstatic\\.com/.*",
        "hints": ["*"]
    },
    {
        "domain": "plausible.io",
        "hints": ["*"]
    }]
}